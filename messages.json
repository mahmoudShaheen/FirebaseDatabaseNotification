/*
 *
 * example of how messages class should be
 *
*/

{
  "messages": {
    "$id": {
      "tokens": //receivers tokens as json ,
      "payload":{
        //message payload as json
      },
      "options":{
        //message options as json can be null
      }
    }
  }
}

//example
{
  "messages": {
    "$id": {
      "tokens": ["token_1","token_2"],
      "payload":{
        "data": {
          "title": "message title",
          "message": "message body",
          "extra_data": "extra data"
        }
      },
      "options":{
        "priority": "high",
        "timeToLive": 86400
      }
    }
  }
}
